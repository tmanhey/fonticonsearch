{
  "name": "fonticonsearch/laravel-fontawesome",
  "description": "Laravel helper for FontAwesome icons",
  "authors": [
    {
      "name": "Joshua de Gier",
      "email": "info@pendo.nl"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "PendoNL\\LaravelFontAwesome\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PendoNL\\LaravelFontAwesome\\Test\\": "tests"
    }
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "FontIconSearch\\LaravelFontAwesome\\LaravelFontAwesomeServiceProvider"
      ],
      "aliases": {
        "FontAwesome": "FontIconSearch\\LaravelFontAwesome\\Facade"
      }
    }
  }
}
